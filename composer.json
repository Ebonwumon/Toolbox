{
    "name": "depotwarehouse/toolbox",
    "description": "",
    "authors": [
        {
            "name": "Troy Pavlek",
            "email": "troy@depotwarehouse.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/database": "4.2.*",
        "illuminate/pagination": "dev-reduce-deps"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "0.6.*"
