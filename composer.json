{
    "name": "depotwarehouse/toolbox",
    "description": "A set of tools in PHP",
    "authors": [
        {
            "name": "Troy Pavlek",
            "email": "troy@depotwarehouse.net"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/validation": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9.4",
        "satooshi/php-coveralls": "^0.6.1"
    },
    "autoload": {
        "psr-4": {
            "Depotwarehouse\\Toolbox\\": "src/"
        },
        "files": [
            "src/Strings/Strings.php",
            "src/Verification/Verification.php",
            "src/Operations/Operations.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Depotwarehouse\\Toolbox\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
